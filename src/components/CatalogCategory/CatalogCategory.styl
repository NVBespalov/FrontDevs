.categoryTitle
  display flex
  align-items center
  .title
    margin 9px
  .delimiter
    flex 1
    display block
    height 1px
    border 0
    border-top 1px solid #ccc
    margin 1em 0
    padding 0
.count
  display flex
  justify-content flex-end
  flex-direction column
  color #A99D7D
  .nav
    width 10px
    height 10px
    &:before
      content '<'
      margin-right 9px
    &:after
      content '>'
.items
  display flex
  .wrapper
    flex 1 1 33%

